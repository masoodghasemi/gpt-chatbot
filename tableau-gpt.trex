<?xml version="1.0" encoding="utf-8"?> 
  <manifest manifest-version="0.1" xmlns="http://www.tableau.com/xml/extension_manifest">
    <dashboard-extension id="com.roche.gptchatbot" extension-version="1.0.0">
      <default-locale>en_US</default-locale>
      <name resource-id="name"/>
      <description>A Tableau extension that connects GPT with dashboard data</description>
      <author name="Masood Ghasemi" email="masood.ghasemi@roche.com" organization="Roche" website="https://masoodghasemi.github.io/gpt-chatbot/"/>
      <min-api-version>1.0</min-api-version>
      <source-location>
        <url>https://masoodghasemi.github.io/gpt-chatbot/index.html</url> 
      </source-location>
      <icon></icon>
      <permissions>
        <permission>full data</permission>
      </permissions>
    </dashboard-extension>
    <resources>
      <resource id="name">
        <text locale="en_US">gpt-chatbot-new</text>
      </resource>
    </resources>
  </manifest>
